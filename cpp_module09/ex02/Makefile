NAME 		= PmergeMe

CC			= c++ -std=c++98
FLAGS		= -Wall -Wextra -Werror
RM			= rm -rf

OBJDIR      = .objFiles

FILES		= main PmergeMe

SRC			= $(FILES:=.cpp)
OBJ			= $(addprefix $(OBJDIR)/, $(FILES:=.o))
HEADER		= PmergeMe.hpp

NONE="\033[0m"
GREEN="\033[32m"
GRAY="\033[2;37m"
CURSIVE="\033[3m"
YELLOW=	"\033[1;33"

.PHONY: all clean fclean re leaks

all: $(NAME)

$(NAME): $(OBJ) $(HEADER)
	@echo $(CURSIVE)$(GRAY) "     - Making object files..." $(NONE)
	@echo $(CURSIVE) $(GRAY) "     - Compiling $(NAME)..." $(NONE)
	@$(CC)  $(FLAGS) $(OBJ) $(OPTS) -o $(NAME)
	@echo $(GREEN)"- Compiled -"$(NONE)

$(OBJDIR)/%.o: %.cpp $(HEADER)
	@mkdir -p $(dir $@)
	@$(CC) $(FLAGS) $(OPTS) -c $< -o $@

play:re
	@./$(NAME) 11 2 17 0 16 8 6 15 10 3 21 1 18 9 14 19 12 5 4 20 13 7

test:re
	@./$(NAME) `shuf -i 1-100000 -n 3000 | tr "\n" " "`

small:re
	@./$(NAME) 9 1 2

clean:
	@$(RM) $(OBJDIR) $(OBJ)

fclean: clean
	@$(RM) $(NAME)

re: fclean all

last:re
	@./$(NAME) 23183 86372 22162 510 87474 79134 13600 42986 24641 23468 19709 28437 25960 40517 80168 16766 97349 2352 23448 99910 49455 9717 86950 9372 25055 75373 42626 14222 75002 78353 67859 23189 67553 8913 24501 1805 87834 66626 90092 84889 26024 61014 89702 2854 72807 1770 45513 5947 94751 46896 73676 79990 6402 80672 77760 48732 18799 64982 42479 48113 28162 74024 75996 15861 79821 87606 56216 50433 40574 52396 12631 44242 22834 56890 45312 37232 6657 44816 34742 97551 91850 21939 998 19025 11903 22413 64032 82361 68198 50550 92395 54211 2015 42714 24650 49448 56098 72292 49213 51103 5941 99113 12549 8236 56367 90 89611 65092 30788 53216 2447 93038 4329 22356 89527 54167 67578 43345 45742 75969 76260 41822 86320 90670 36947 40418 62047 94989 786 98258 91553 50059 71219 10354 25467 82809 72328 58094 80325 59100 80301 81743 5023 97221 88693 87068 67248 25957 41390 66425 98996 83489 39604 71182 43138 30147 64336 78651 29428 51406 9233 66351 19445 64374 89492 77343 97170 74893 61807 26296 60323 30615 97147 6849 1541 20679 96481 97141 63444 67100 56072 84258 75745 91440 38214 86183 59211 65791 54312 47014 95456 98229 88044 10474 51708 54628 32216 23747 10582 35672 11957 36198 8820 63297 89809 84654 66816 43296 2159 56847 74581 26123 65054 73399 35964 5125 21067 95964 3453 49420 72208 79980 54814 12639 51017 78829 8066 37853 94433 91386 2858 74524 19019 89062 16654 48931 85702 13703 64520 34196 72635 50619 83499 3471 33416 89897 33611 81216 93552 12770 51569 77407 16903 54317 29598 85138 41569 66209 78601 13280 4332 56372 60800 9366 65778 34299 90941 57460 98901 9802 19967 80997 62596 48893 29160 41372 14980 70999 78561 49680 57491 97321 79976 27378 77615 25435 74132 4697 5124 98458 59806 44720 97644 14612 26817 45674 86466 94129 88315 15041 4409 20682 77575 79074 73840 26607 20916 94649 40513 21483 11721 52721 25398 42366 89056 20688 74119 19227 15962 21982 8943 43770 58812 25876 29222 57563 32620 87802 8460 59140 14252 31171 78989 79255 75164 95062 52627 90271 942